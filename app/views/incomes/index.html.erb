<div class="container">
  <h1>Entradas</h1>
  <%= render partial: 'form' %>

  <div class="table-responsive">
    <table class="table table-striped table-bordered table-hover">
      <thead>
        <tr>
          <th>Data</th>
          <th>Categoria</th>
          <th>Descrição</th>
          <th>Valor</th>
          <th>Forma de Pagamento</th>
          <th>Usuário</th>
        </tr>
      </thead>

      <tbody>
        <% @incomes.reverse.each do |income| %>
         <%= render partial: 'income', locals: {income: income} %>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
